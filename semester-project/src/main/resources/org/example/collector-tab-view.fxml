<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="org.example.controllers.CollectorController"
            minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
    <children>
        <ProgressBar fx:id="progressBar" layoutX="184.0" layoutY="321.0" prefHeight="18.0" prefWidth="271.0"
        />
        <TextArea fx:id="urlListInput" layoutX="172.0" layoutY="62.0" prefWidth="296.0" promptText="Picture API URLs"/>
        <Button layoutX="256.0" layoutY="258.0" mnemonicParsing="false" prefHeight="31.0"
